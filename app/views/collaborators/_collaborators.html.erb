<h4>Collaborators:</h4>
<ul>
  <% @wiki.collaborators.each do |collaborator| %>
    <li>
      <h4><%= collaborator.user.email %></h4>
      <% if current_user.premium? || current_user.admin? %>
        <%= link_to "Delete Collaborator", wiki_collaborator_path(@wiki, @wiki.collaborators.find_by_user_id(collaborator.user.id).id), method: :delete, class: 'btn btn-danger' %>
      <% end %>
    </li>
  <% end %>
</ul>
